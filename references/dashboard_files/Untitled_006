function addTalkButton() { if (null !== document.getElementById("mobile_support_page")) {var t = document.createElement("a");(t.className = "button"),  (t.style.display = "none"),  t.setAttribute("data-role", "button"),  t.setAttribute("data-sm-show-media-selection-on", "click"),  (t.href = "javascript:void (0);"),  (t.innerText = window.Alkami.Localization.SiteText["Glia.Mobile.LetsTalk"] ? window.Alkami.Localization.SiteText["Glia.Mobile.LetsTalk"] : "Let\'s Talk!");var e = document.getElementById("support_large_icon");e && e.insertAdjacentHTML("afterend", t.outerHTML); }  }  function startAlkamiGliaConnection() { sm.getApi({ version: "v1" }).then(function (t) {t.addEventListener(t.EVENTS.QUEUE_STATE_UPDATE, function (t) {  var display = t.state === t.QUEUE_STATES.CAN_QUEUE ? "" : "none";  const e = document.querySelector("[data-sm-show-media-selection-on]");  if (e && e.style.display !== display) e.style.display = display;}); }); const evt = new CustomEvent('completeAlkamiGliaConnection'); document.body.dispatchEvent(evt);} addTalkButton(); setTimeout(() => {startAlkamiGliaConnection(); }, 700);